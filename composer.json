{
    "name": "metglobal/utils",
    "description": "Compass php utility helpers",
    "type": "library",
    "keywords": [
        "utility",
        "utils",
        "helper"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "burak.bolat@metglobal.com"
        }
